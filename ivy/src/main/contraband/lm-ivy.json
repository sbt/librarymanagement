{
  "codecNamespace": "sbt.librarymanagement",
  "types": [
    {
      "name": "IvyConfiguration",
      "namespace": "sbt.librarymanagement.ivy",
      "target": "Scala",
      "type": "interface",
      "fields": [
        { "name": "log",           "type": "xsbti.Logger"                                                                                                      },
        { "name": "lock",          "type": "xsbti.GlobalLock?", "since": "0.0.1",                       "default": "scala.None"                                },
        { "name": "baseDirectory", "type": "java.io.File", "since": "0.0.1",                            "default": "new java.io.File(\".\")"                   },
        { "name": "ivyHome",       "type": "java.io.File?", "since": "0.0.1",                           "default": "scala.None"                                },
        { "name": "updateOptions", "type": "sbt.librarymanagement.ivy.UpdateOptions", "since": "0.0.1", "default": "sbt.librarymanagement.ivy.UpdateOptions()" }
      ],
      "types": [
        {
          "name": "InlineIvyConfiguration",
          "namespace": "sbt.librarymanagement.ivy",
          "target": "Scala",
          "type": "record",
          "fields": [
            { "name": "resolvers",            "type": "sbt.librarymanagement.Resolver*"           , "since": "0.0.1", "default": "sbt.librarymanagement.Resolver.defaultResolvers(Vector.empty, true, true).toVector" },
            { "name": "otherResolvers",       "type": "sbt.librarymanagement.Resolver*"           , "since": "0.0.1", "default": "scala.collection.immutable.Vector.empty"                                            },
            { "name": "moduleConfigurations", "type": "sbt.librarymanagement.ModuleConfiguration*", "since": "0.0.1", "default": "scala.collection.immutable.Vector.empty"                                            },
            { "name": "checksums",            "type": "String*"                                   , "since": "0.0.1", "default": "scala.collection.immutable.Vector.empty // Or IvySbt.DefaultChecksums?\n"           },
            { "name": "managedChecksums",     "type": "Boolean"                                   , "since": "0.0.1", "default": "false"                                                                              },
            { "name": "resolutionCacheDir",   "type": "java.io.File?"                             , "since": "0.0.1", "default": "scala.None"                                                                         }
          ]
        },
        {
          "name": "ExternalIvyConfiguration",
          "namespace": "sbt.librarymanagement.ivy",
          "target": "Scala",
          "type": "record",
          "fields": [
            { "name": "uri",            "type": "java.net.URI"                    },
            { "name": "extraResolvers", "type": "sbt.librarymanagement.Resolver*" }
          ]
        }
      ]
    }
  ]
}
